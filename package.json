{
  "name": "lycophron",
  "version": "0.1.0",
  "description": "A stupid anagram solver with a few sample applications.",
  "bin": {},
  "main": "./lib/lycophron.js",
  "scripts": {
    "start": "node server",
    "test": "node node_modules/mocha/bin/mocha --recursive test",
    "test_cover_win": "node_modules\\.bin\\istanbul.cmd --hook-run-in-context cover node_modules\\mocha\\bin\\_mocha -- -R spec --recursive test",
    "test_cover": "node node_modules/istanbul/lib/cli.js --hook-run-in-context cover node_modules/mocha/bin/_mocha -- -R spec --recursive test",
