  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lycophron/lycophron.git"
  },
  "keywords": [
    "anagram",
    "words",
    "game",
    "multiplayer",
    "competition"
  ],
  "author": "Zsolt Lattmann <zsolt.lattmann@gmail.com>",
  "bugs": {
    "url": "https://github.com/lycophron/lycophron/issues"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "=2.7.1",
    "compression": "=1.4.3",
    "cookie-parser": "=1.3.4",
    "debug": "=2.1.3",
    "express": "=4.12.3",
    "express-session": "=1.10.4",
    "glob": "=5.0.3",
    "nedb": "=1.1.1",
    "passport": "=0.2.1",
    "passport-google-oauth": "=0.2.0",
    "socket.io": "=1.3.5",
    "superagent": "=1.1.0",
    "winston": "=0.9.0"
